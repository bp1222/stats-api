{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.8.0",
    "generators": {
      "v0-ts-client": {
        "generatorName": "typescript-fetch",
        "inputSpec": "bundle.yaml",
        "output": "typescript/src",
        "additionalProperties": {
          "withInterfaces": true
        }
      },
      "v0-go-client": {
        "generatorName": "go",
        "inputSpec": "bundle.yaml",
        "output": "go",
        "gitUserId": "bp1222",
        "gitRepoId": "stats-api",
        "additionalProperties": {
          "packageName": "go",
          "isGoSubmodule": true
        }
      }
    }
  }
}